{
	"private": true,
	"name": "apollo",
	"version": "7.0.0",
	"pre-commit": [
		"lint"
	],
	"scripts": {
		"dev": "npm run lint && tsnd --project tsconfig.server.json --ignore-watch .next server/main.ts",
		"build": "npm run lint && rimraf .next && run-p build:server build:client",
		"build:server": "tsc --project tsconfig.server.json",
		"build:client": "next build",
