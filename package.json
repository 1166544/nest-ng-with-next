    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "ky": "^0.19.0",
    "ky-universal": "^0.5.0",
    "next": "^9.3.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13",
